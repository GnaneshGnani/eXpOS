decl
	int isPrime(int n);
enddecl

int isPrime(int n)
{
	decl
		int i,temp;
	enddecl
	begin
		i = 2;
		temp = 1;
		while(i < n)
		do
			if((n % i) == 0)
			then
				temp = 0;
				break;
			endif;
			i = i + 1;
		endwhile;
		return temp;
	end
}

int main()
{
	decl
		int n,temp;
	enddecl
	begin
		n = 2;
		while(n <= 100)
		do
			temp = isPrime(n);
			if(temp == 1)
			then
				write("Prime");
				write(n);
			endif;
			n = n + 1;
		endwhile;
		return 0;
	end
}
